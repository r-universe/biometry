[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "tapnet"]
	path = tapnet
	url = https://github.com/biometry/tapnet
	shallow = true
	subdir = tapnet
	registered = true
[submodule "bipartite"]
	path = bipartite
	url = https://github.com/biometry/bipartite
	shallow = true
	subdir = bipartite
	registered = true
